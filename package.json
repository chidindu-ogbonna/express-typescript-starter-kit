{
  "name": "express-typescript-boilerplate",
  "version": "1.0.14",
  "description": "An Express boilerplate backend",
  "author": "Edwin Hernandez",
  "repository": "edwinhern/express-typescript-2024",
  "license": "MIT",
  "main": "index.ts",
  "private": true,
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/index.ts | pino-pretty",
    "dev:langgraph": "npx @langchain/langgraph-cli dev",
    "build": "tsup",
    "start": "node dist/index.js",
    "clean": "rimraf dist coverage",
    "lint": "biome check src/",
    "lint:fix": "biome check src/ --fix",
    "format": "biome format src/",
    "test": "vitest run",
    "prepare": "husky"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^0.0.55",
    "@asteasolutions/zod-to-openapi": "^7.0.0",
    "ai": "^3.4.33",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "helmet": "^8.0.0",
    "http-status-codes": "^2.3.0",
    "pino": "^9.4.0",
    "pino-http": "^10.0.0",
    "swagger-ui-express": "^5.0.0",
    "@langchain/anthropic": "^0.3.3",
    "@langchain/aws": "^0.1.0",
    "@langchain/cohere": "^0.3.0",
    "@langchain/community": "^0.3.27",
    "@langchain/core": "^0.3.36",
    "@langchain/google-genai": "^0.1.0",
    "@langchain/google-vertexai": "^0.1.0",
    "@langchain/groq": "^0.1.2",
    "@langchain/langgraph": "^0.2.11",
    "@langchain/langgraph-sdk": "^0.0.14",
    "@langchain/mistralai": "^0.1.1",
    "@langchain/ollama": "^0.1.0",
    "@langchain/openai": "^0.3.5",
    "duck-duck-scrape": "^2.2.7",
    "langchain": "^0.3.2",
    "langsmith": "^0.1.61",
    "ts-node": "^10.9.2",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@eslint/js": "^9.14.0",
    "@types/cors": "^2.8.17",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "pino-pretty": "^11.0.0",
    "rimraf": "^6.0.0",
    "supertest": "^7.0.0",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0",
    "vite-tsconfig-paths": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,json,jsonc}": [
      "biome check --apply --no-errors-on-unmatched"
    ]
  },
  "tsup": {
    "entry": [
      "src",
      "!src/**/__tests__/**",
      "!src/**/*.test.*"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  }
}
